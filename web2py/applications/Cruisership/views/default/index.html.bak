        function GetSessionValues(flag) {
            showBlockPanel();
            var JType;
            if (document.getElementById("hdMasterType").value != null) {
                JType = document.getElementById("hdMasterType").value;
            }

            var JLines;
            if (document.getElementById("hdMasterLines").value != null) {
                JLines = document.getElementById("hdMasterLines").value;
            }
            if (JType.indexOf("all")>=0) {//To  remove all value from url when select all option is selected

                JLines = "";
                JType = "";
            }
            var JDestination;
            if (document.getElementById("hdMasterDestination").value != null) {
                JDestination = document.getElementById("hdMasterDestination").value;
            }
            if (JDestination.indexOf("all")>=0) {//To remove all value from url when select all option is selected
                JDestination = "";
            }
            var JDateRange;
            if (document.getElementById("hdMasterDateRangeCriteria").value != null) {
                JDateRange = document.getElementById("hdMasterDateRangeCriteria").value;
            }
            var JDateRangeCriteria;
            if (document.getElementById("hdMasterDateRange").value != null) {
                JDateRangeCriteria = document.getElementById("hdMasterDateRange").value;
            }
            var JDealDateRange;
            if (document.getElementById("hdMasterDealDateRange").value != null) {
                JDealDateRange = document.getElementById("hdMasterDealDateRange").value;
            }
            var stringLenght = (JType + JLines + JDestination + JDateRange + JDateRangeCriteria + JDealDateRange).length;
            var op = document.getElementById("hdCount").value
            var All = "";
            if (stringLenght <= op) {
                //Index.aspx is removed by samy from this url for not showing it in the url
                if (stringLenght == 0) {
                    All = "All";
                    window.location.href = '/search/' + "?values=" + JType + ":" + JLines + ":" + JDestination + ":" + JDateRangeCriteria + ":" + JDateRange + ":" + JDealDateRange + ":" + All;
                }
                else {
                    All = "CruiseSearch";
                    window.location.href = '/search/' + "?values=" + JType + ":" + JLines + ":" + JDestination + ":" + JDateRangeCriteria + ":" + JDateRange + ":" + JDealDateRange + ":" + All;
                }
                return false;
            }
            else {
                alert('Max Character Exceeds');
                return true;
            }
            hideBlockPanel();
        }
